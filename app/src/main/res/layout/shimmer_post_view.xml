    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >
            <ImageView

                android:layout_width="@dimen/dp_40"
                android:layout_height="@dimen/dp_40"
                android:background="@drawable/curved_small_square_box_light_grey"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginVertical="@dimen/dp_11"
                android:scaleType="centerCrop"
                android:clipToOutline="true"
                android:layout_centerVertical="true"
                />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/dp_55"
                android:gravity="center_vertical"
                android:layout_centerVertical="true"

                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center_vertical"
                    android:layout_marginBottom="@dimen/dp_5"
                    >
                    <TextView

                        android:layout_width="@dimen/dp_105"
                        android:layout_height="@dimen/dp_10"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:textSize="@dimen/sp_18"
                        android:background="@drawable/curved_small_square_box_light_grey"/>


                </LinearLayout>
                <TextView

                    android:layout_width="@dimen/dp_105"
                    android:layout_height="@dimen/dp_10"
                    android:textSize="@dimen/sp_14"
                    android:layout_marginTop="2dp"
                    android:textColor="@color/black"
                    android:background="@drawable/curved_small_square_box_light_grey"/>

            </LinearLayout>

            <ImageView
                android:layout_width="@dimen/dp_45"
                android:layout_height="@dimen/dp_20"
                android:background="@drawable/curved_small_square_box_light_grey"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>

        </RelativeLayout>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/dp_400"
            android:scaleType="fitStart"
            android:background="@color/light_grey_text"
            android:adjustViewBounds="true"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_48"

            >

            <LinearLayout
                android:layout_width="@dimen/dp_125"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_5"
                android:background="@drawable/curved_small_square_box_light_grey">


            </LinearLayout>

            <ImageView

                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_16"
                android:background="@color/light_grey_text"/>

        </RelativeLayout>
   <LinearLayout
